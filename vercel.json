{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, stale-while-revalidate=31536000"
                }
            ]
        },
        {
            "source": "/(.*\\.html$)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, no-cache, must-revalidate"
                }
            ]
        }
    ]
}